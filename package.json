{
  "name": "laravel-shopping-cart",
  "version": "2.0.0",
  "description": "A simple shopping cart implementation for Laravel",
  "private": true,
  "type": "module",
  "workspaces": [
    "docs"
  ],
  "scripts": {
    "docs:dev": "npm run docs:dev --workspace=docs",
    "docs:build": "npm run docs:build --workspace=docs",
    "docs:preview": "npm run docs:preview --workspace=docs",
    "docs:install": "npm install --workspace=docs",
    "docs:clean": "npm run clean --workspace=docs",
    "install:all": "npm install && npm install --workspace=docs",
    "build:all": "npm run docs:build",
    "clean:all": "npm run docs:clean && rm -rf node_modules",
    "dev": "npm run docs:dev"
  },
  "keywords": [
    "laravel",
    "shopping-cart",
    "ecommerce",
    "cart",
    "php",
    "package"
  ],
  "author": "Prasit Gebsaap",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/soap/laravel-shopping-cart.git"
  },
  "bugs": {
    "url": "https://github.com/soap/laravel-shopping-cart/issues"
  },
  "homepage": "https://soap.github.io/laravel-shopping-cart/",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0"
  }
}